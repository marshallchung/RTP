{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "arcanedev/log-viewer": "^10.0",
        "bacon/bacon-qr-code": "^3.0",
        "barryvdh/laravel-dompdf": "^2.0",
        "creativeorange/gravatar": "~1.0",
        "doctrine/dbal": "*",
        "endroid/qr-code": "^6.0",
        "google/recaptcha": "^1.3",
        "graham-campbell/throttle": "^10.0",
        "guzzlehttp/guzzle": "^7.2",
        "laracasts/flash": "^3.2",
        "laravel/framework": "^10.10",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.10",
        "laravel/ui": "*",
        "laravelcollective/html": "*",
        "lavary/laravel-menu": "^1.0",
        "livewire/livewire": "^2.12",
        "maatwebsite/excel": "^3.1.48",
        "marktopper/doctrine-dbal-timestamp-type": "^1.1",
        "michaelachrisco/readonly": "^0.60.0",
        "nesbot/carbon": "*",
        "nette/schema": "^1.2",
        "pragmarx/google2fa": "^8.0",
        "pragmarx/google2fa-qrcode": "^3.0",
        "predis/predis": "^2.1",
        "rutorika/sortable": "^9.1",
        "santigarcor/laratrust": "^8.2",
        "spatie/laravel-activitylog": "^4.7",
        "spatie/laravel-csp": "^2.8",
        "spatie/laravel-sitemap": "^6.3",
        "yajra/laravel-datatables": "^10.0",
        "yajra/laravel-datatables-buttons": "^10.0",
        "yajra/laravel-datatables-html": "^10.6",
        "yajra/laravel-datatables-oracle": "^10.4",
        "zanysoft/laravel-zip": "^2.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "recca0120/laravel-tracy": "^1.8.14",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
